#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.45.6_vite@6.4.1_@types_ecb29cada8c5ee7f58bd01521788c031/node_modules/@sveltejs/kit/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.45.6_vite@6.4.1_@types_ecb29cada8c5ee7f58bd01521788c031/node_modules/@sveltejs/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.45.6_vite@6.4.1_@types_ecb29cada8c5ee7f58bd01521788c031/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.45.6_vite@6.4.1_@types_ecb29cada8c5ee7f58bd01521788c031/node_modules/@sveltejs/kit/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.45.6_vite@6.4.1_@types_ecb29cada8c5ee7f58bd01521788c031/node_modules/@sveltejs/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@6.2.1_svelte@5.45.6_vite@6.4.1_@types_ecb29cada8c5ee7f58bd01521788c031/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
