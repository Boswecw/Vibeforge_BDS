#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/vitest@4.0.15_@types+node@22.19.1_@vitest+ui@4.0.15_jiti@2.6.1_jsdom@27.3.0_postcss@8.5.6__lightningcss@1.30.2/node_modules/vitest/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/vitest@4.0.15_@types+node@22.19.1_@vitest+ui@4.0.15_jiti@2.6.1_jsdom@27.3.0_postcss@8.5.6__lightningcss@1.30.2/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/vitest@4.0.15_@types+node@22.19.1_@vitest+ui@4.0.15_jiti@2.6.1_jsdom@27.3.0_postcss@8.5.6__lightningcss@1.30.2/node_modules/vitest/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/vitest@4.0.15_@types+node@22.19.1_@vitest+ui@4.0.15_jiti@2.6.1_jsdom@27.3.0_postcss@8.5.6__lightningcss@1.30.2/node_modules:/home/charles/projects/Coding2025/Forge/vibeforge_bds/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@4.0.15_@types+node@22.19.1_@vitest+ui@4.0.15_jiti@2.6.1_jsdom@27.3.0_postcss@8.5.6__lightningcss@1.30.2/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@4.0.15_@types+node@22.19.1_@vitest+ui@4.0.15_jiti@2.6.1_jsdom@27.3.0_postcss@8.5.6__lightningcss@1.30.2/node_modules/vitest/vitest.mjs" "$@"
fi
